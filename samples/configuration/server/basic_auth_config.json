{
  "ip": "0.0.0.0",
  "port": 8080,
  "auth": {
    "basic": {
      "etcd": {
        "urls": "http://etcd:2379",
        "tls": {
          "root_certificate": "etcd-ca.crt",
          "identity": {
            "certificate": "etcd-client.crt",
            "key": "etcd-client.key"
          }
        },
        "path": "/users",
        "data_format": "json",
        "lease_timeout": {
          "secs": 60,
          "nanos": 0
        },
        "connect_timeout": {
          "secs": 30,
          "nanos": 0
        },
        "cache": {
          "size": 10,
          "usage": {
            "period": {
              "secs": 60,
              "nanos": 0
            },
            "evicted_threshold": 10
          }
        }
      },
      "cache": {
        "size": 10,
        "usage": {
          "period": {
            "secs": 60,
            "nanos": 0
          },
          "evicted_threshold": 10
        }
      }
    }
  },
  "out_stream": {
    "url": "pub+bind:ipc:///tmp/server",
    "send_timeout": {
      "secs": 1,
      "nanos": 0
    },
    "send_retries": 3,
    "receive_timeout": {
      "secs": 1,
      "nanos": 0
    },
    "receive_retries": 3,
    "send_hwm": 1000,
    "receive_hwm": 1000,
    "fix_ipc_permissions": 511
  }
}
